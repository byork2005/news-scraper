<div class="scrapes">
    <div>
        {{#each Data}}
        <div class="each-article row">
            <div class="col-md-8">
                <p>{{title}}</p>
                <p>
                    <a>{{link}}</a>
                </p>
            </div>
            <div class="col-md-1 col-md-offset-3 save-btn">
                <a onclick="saveArticle()" class="btn btn-success" data-title="{{title}}" data-link="{{link}}" data-saved="false">Save</a>
            </div>
        </div>
        <hr> {{/each}}
    </div>

</div>